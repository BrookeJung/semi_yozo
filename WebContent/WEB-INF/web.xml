<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>SemiProject</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>com.que_board.controller.recipeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/recipeController.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ZzimController</servlet-name>
    <servlet-class>com.Zzim.controller.ZzimController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ZzimController</servlet-name>
    <url-pattern>/ZzimController.do</url-pattern>
  </servlet-mapping>
</web-app>